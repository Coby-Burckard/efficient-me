on: absolute;\r\n  width: 100%;\r\n  margin: 0;\r\n  padding: 0;\r\n  background-color: #6494ed;\r\n  opacity: 0.7;\r\n  color: white;\r\n  justify-content: center;\r\n  transition: 0.5s;\r\n}\r\n\r\n.home-body > * {\r\n  text-align: center;\r\n  margin: 0;\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n}\r\n\r\n.home-body p {\r\n  padding: 0.5rem;\r\n  margin-left: 20%;\r\n  margin-right: 20%;\r\n  word-wrap: normal;\r\n}\r\n\r\n.get-started-button a {\r\n  text-decoration: none;\r\n  padding: 0.25rem;\r\n  color: black;\r\n  background-color: white;\r\n}\r\n\r\n/* tab body css */\r\n\r\n.ta-goal-container {\r\n  box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);\r\n  transition: 0.3s;\r\n  padding: 1rem;\r\n  margin: 2rem;\r\n  max-width: 80%;\r\n}\r\n\r\n.ta-goal-container:hover {\r\n  box-shadow: 0 8px 16px 0 rgba(0, 0, 0, 0.2);\r\n}\r\n\r\n.TA-list-box {\r\n  position: relative;\r\n  overflow: hidden;\r\n  max-height: 0px;\r\n}\r\n\r\n.goal-form input {\r\n  display: block;\r\n}\r\n\r\n.goal-form label {\r\n  display: inline-block;\r\n}\r\n\r\n/* delete confirmation */\r\n#delete-popup {\r\n  padding: 2rem;\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  padding: 2rem 3rem;\r\n}\r\n\r\n#delete-popup div {\r\n  padding: 0.5rem 0;\r\n}\r\n\r\n/* universal features */\r\n\r\n.hidden {\r\n  display: none;\r\n}\r\n\r\n.error {\r\n  color: red;\r\n}\r\n\r\n.close {\r\n  position: absolute;\r\n  top: 2px;\r\n  right: 2px;\r\n}\r\n\r\n.expand {\r\n  max-height: 100%;\r\n}\r\n\r\n/* messing around */\r\n\r\n.user-tab {\r\n  display: flex;\r\n}\r\n\r\n.graph-body {\r\n  width: 50%;\r\n}\r\n\r\n.goal-body {\r\n  width: 50%;\r\n}\r\n"},"lineCount":214}},"error":null,"hash":"d3af9da74662bd966a2bc0a9e90940bc","cacheData":{"env":{}}}